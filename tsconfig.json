{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "main.d.ts",
    "src/**/*",
    "src/**/*.vue",
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "vite/client"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
    },
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}